<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RatingBar">
        <!--尺寸值-->
        <attr name="starSize" format="dimension" />
        <!--星星间距-->
        <attr name="starMargin" format="dimension" />
        <!--星星总数-->
        <attr name="starCount" format="integer" />
        <!--星星-->
        <attr name="star" format="reference"/>
    </declare-styleable>

    <declare-styleable name="StarBar">
        <!--星星间距-->
        <attr format="dimension" name="starDistance"/>
        <!--星星大小-->
        <attr format="dimension" name="size"/>
        <!--星星个数-->
        <attr format="integer" name="count"/>
        <!--星星空图-->
        <attr format="reference" name="starEmpty"/>
        <!--星星满图-->
        <attr format="reference" name="starFill"/>
    </declare-styleable>
</resources>